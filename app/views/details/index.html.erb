<h1>Details</h1>

<% @details.each do |d| %>
  <% d.accommodations.each do |a|%>
    <p><%=a.name%></p>
  <% end %>
<% end %>

 <%= form_with(model: @accommodation) do |form| %>
 
 <div>

 <% form.label :name%>
 
 <%= form.text_field :name, placeholder: "Where are you staying?"%> 
 <br>
 <br>
 <%= form.submit "Submit", class:"btn btn-outline-primary" %>

 </div>
 <% end %>

